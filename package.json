{
  "name": "publish50",
  "version": "0.1.0",
  "description": "Automates video transcoding and publishing",
  "author": "CS50",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.401.0",
    "bootstrap": "^4.2.1",
    "bull": "^3.6.0",
    "electron-log": "^3.0.1",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.0.6",
    "fs-extra": "^7.0.1",
    "imagemagick": "^0.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "react": "^16.8.1",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redis": "^2.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && ELECTRON_DEV=1 electron .\"",
    "electron-pack": "yarn build && build -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "wait-on": "^3.2.0"
  },
  "main": "public/electron.js",
  "build": {
    "productName": "publish50",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": "bin/**/*",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.video",
      "icon": "build/icons/512x512.png"
    },
    "linux": {
      "icon": "build/icons/512x512.png"
    }
  },
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cs50/publish50-electron.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cs50/publish50-electron/issues"
  },
  "publish": {
    "provider": "github",
    "owner": "CS50"
  }
}
