{
  "name": "publish50",
  "version": "0.3.2",
  "description": "Automates video transcoding and publishing",
  "author": "CS50",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.401.0",
    "bootstrap": "^4.3.1",
    "bull": "^3.6.0",
    "electron-log": "^4.0.4",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.0.6",
    "fs-extra": "^8.1.0",
    "google-auth-library": "^5.9.2",
    "googleapis": "^47.0.0",
    "jquery": "^3.4.0",
    "node-fetch": "^2.3.0",
    "popper.js": "^1.14.7",
    "react": "^16.12.0",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redis": "^2.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently 'BROWSER=none yarn start' 'wait-on http://localhost:3000 && ELECTRON_DEV=1 electron .'",
    "dist": "yarn electron-builder && electron-builder -c.extraMetadata.main=build/electron.js",
    "release": "electron-builder -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "wait-on": "^4.0.0"
  },
  "main": "public/electron.js",
  "build": {
    "productName": "publish50",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": "bin/**/*",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.video",
      "icon": "build/icons/512x512.png"
    },
    "linux": {
      "icon": "build/icons/512x512.png"
    }
  },
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cs50/publish50-electron.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cs50/publish50-electron/issues"
  },
  "publish": {
    "provider": "github",
    "owner": "CS50"
  }
}
