{
  "name": "publish50",
  "version": "0.3.8",
  "description": "Automates video transcoding and publishing",
  "author": "CS50",
  "private": true,
  "dependencies": {
    "@babel/helper-define-map": "^7.15.4",
    "@babel/helper-regex": "^7.10.5",
    "aws-sdk": "^2.991.0",
    "bootstrap": "^4.6.0",
    "bull": "^3.29.2",
    "color-string": "^1.6.0",
    "dns-packet": "^1.3.4",
    "electron-log": "^4.4.1",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.3.9",
    "elliptic": "^6.5.4",
    "fs-extra": "^10.0.0",
    "google-auth-library": "^7.9.2",
    "googleapis": "^87.0.0",
    "http-proxy": "^1.18.1",
    "jquery": "^3.6.0",
    "node-fetch": "^2.6.2",
    "path-parse": "^1.0.7",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redis": "^3.1.2",
    "websocket-extensions": "^0.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently 'BROWSER=none yarn start' 'wait-on http://localhost:3000 && ELECTRON_DEV=1 electron .'",
    "dist": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js",
    "release": "electron-builder -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^15.5.5",
    "electron-builder": "^22.11.7",
    "wait-on": "^6.0.0"
  },
  "main": "public/electron.js",
  "build": {
    "productName": "publish50",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": "bin/**/*",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.video",
      "icon": "build/icons/512x512.png"
    },
    "linux": {
      "icon": "build/icons/512x512.png"
    }
  },
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cs50/publish50-electron.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cs50/publish50-electron/issues"
  },
  "publish": {
    "provider": "github",
    "owner": "CS50"
  }
}
